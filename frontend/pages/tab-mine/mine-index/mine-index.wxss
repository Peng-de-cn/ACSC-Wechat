page {
  background-clip: #fff;
}
.topImage {
  width: 100%;
  height: 250rpx;
  position: absolute;
  z-index: 0;
}

.topBG {
  background-color: #1887f9;
  height: 250rpx;
  position: relative;
  width: 100%;
  background-image: url('https://i.loli.net/2019/12/31/7mKNsCgWLVqaObZ.jpg');
  background-repeat: no-repeat;
  background-size: cover;
  display: flex;
}

.topBG .left {
  margin-top: 60rpx;
  margin-left: 70rpx;
}

.topBG .left .head {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  border: 6rpx #fff solid;
  margin: 0 auto;
  z-index: 3;
  position: relative;
  box-shadow: 1px 1px 10px #555;
}

.topBG .right {
  margin-top: 76rpx;
  margin-left: 50rpx;
}

.topBG .right .name {
  font-size: 36rpx;
  z-index: 3;
  position: relative;
  color: #fff;
  margin-bottom: 14rpx;
}

.topBG .right .slogan {
  font-size: 28rpx;
  z-index: 3;
  position: relative;
  color: #fff;
}

.city {
  color: #8d8d8d;
  font-size: 30rpx;
  margin-top: 15rpx;
}

.cell-list {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 110rpx;
}

.cell {
  width: 92%;
  height: 100rpx;
  border-bottom: 1px solid rgba(230, 230, 230, 0.6);
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cell-left {
  display: flex;
  height: 100%;
  align-items: center;
}

.cell-icon {
  width: 50rpx;
  height: 50rpx;
}

.cell-icon image {
  width: 50rpx;
  height: 50rpx;
}

.cell-text {
  width: 390rpx;
  font-size: 30rpx;
  color: #414141;
  margin-left: 15rpx;
}

.cell-text-no {
  width: 550rpx;
}

.cell-text button, .cell-text button::after {
  background: none !important;
  border: none !important;
  text-align: left !important;
  padding: 0 !important;
  color: #414141 !important;
  width: 100%;
}

.cell-right {
  color: #8d8d8d;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  margin-right: 20rpx;
}

.cell-right image {
  width: 16rpx;
  height: 24rpx;
  margin-left: 12rpx;
}

button::after, button {
  border: none;
  background: none;
}

.hidden-entrance {
  width: 200rpx;
  height: 200rpx;
  opacity: 0;
  position: relative;
  z-index: 99;
}

.cell-list .cell-text button {
  font-weight: normal;
  font-size: 16px;
}