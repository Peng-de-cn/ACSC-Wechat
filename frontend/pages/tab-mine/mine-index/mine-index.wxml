<view class="center-wrap">
  <view class="topBG">
    <!-- <image class="topImage" src="https://i.loli.net/2019/12/31/7mKNsCgWLVqaObZ.jpg"></image> -->
    <view class="left">
      <view class="avatar">
        <!-- <image class="head" src="https://i.loli.net/2019/12/31/fFgCvRIdjJlBuKN.jpg"></image> -->
      </view>
    </view>
    <view class="right">
      <view class="name">测试帐号</view>
      <!-- <view class="slogan">共同成长，彼此成就</view> -->
    </view>
  </view>
  <view class="cell-list">
    <view bindtap="personal" class="cell">
      <view class="cell-left">
        <view class="cell-icon">
          <image src="/images/personal.png"></image>
        </view>
        <view class="cell-text">
          <text>个人资料</text>
        </view>
      </view>
      <view class="cell-right">
        <image src="/images/right.png"></image>
      </view>
    </view>
    <view bindtap="activities" class="cell">
      <view class="cell-left">
        <view class="cell-icon">
          <image src="/images/activities.png"></image>
        </view>
        <view class="cell-text">
          <text>我的活动</text>
        </view>
      </view>
      <view class="cell-right">
        <image src="/images/right.png"></image>
      </view>
    </view>
    <view bindtap="collection" class="cell">
      <view class="cell-left">
        <view class="cell-icon">
          <image src="/images/ico_collection.png"></image>
        </view>
        <view class="cell-text">
          <text>我的收藏</text>
        </view>
      </view>
      <view class="cell-right">
        <image src="/images/right.png"></image>
      </view>
    </view>
    <view bindtap="join" class="cell">
      <view class="cell-left">
        <view class="cell-icon">
          <image src="/images/join.png"></image>
        </view>
        <view class="cell-text">
          <text>加入会员</text>
        </view>
      </view>
      <view class="cell-right">
        <image src="/images/right.png"></image>
      </view>
    </view>
    <view bindtap="feedback" class="cell">
      <view class="cell-left">
        <view class="cell-icon">
          <image src="/images/feedback.png"></image>
        </view>
        <view class="cell-text">
          <!-- <text>意见反馈</text> -->
          <button openType="feedback" style="width:150%">用户反馈</button>
        </view>
      </view>
      <view class="cell-right">
        <image src="/images/right.png"></image>
      </view>
    </view>
    <view bindtap="callUs" class="cell">
      <view class="cell-left">
        <view class="cell-icon">
          <image src="/images/phone.png"></image>
        </view>
        <view class="cell-text">
          <text>联系我们</text>
        </view>
      </view>
      <view class="cell-right">
        <image src="/images/right.png"></image>
      </view>
    </view>
    <view bindtap="aboutUs" class="cell">
      <view class="cell-left">
        <view class="cell-icon">
          <image src="/images/aboutus.png"></image>
        </view>
        <view class="cell-text">
          <text>关于我们</text>
        </view>
      </view>
      <view class="cell-right">
        <image src="/images/right.png"></image>
      </view>
    </view>
  </view>
</view>